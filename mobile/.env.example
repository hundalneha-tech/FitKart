# Mobile App Configuration

# API Configuration
REACT_APP_API_URL=http://localhost:3000/api
REACT_APP_API_TIMEOUT=30000

# Firebase Configuration (for push notifications)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_PROJECT_ID=your_firebase_project_id
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_firebase_app_id

# Google Fit API (Android)
REACT_APP_GOOGLE_FIT_CLIENT_ID=your_google_fit_client_id

# Apple HealthKit (iOS)
# No additional configuration needed, uses native permissions

# Shopify Configuration
REACT_APP_SHOPIFY_STORE_DOMAIN=your_shopify_store.myshopify.com
REACT_APP_SHOPIFY_ACCESS_TOKEN=your_shopify_access_token

# Environment
NODE_ENV=development
