{
  "name": "fitkart-backend",
  "version": "1.0.0",
  "description": "FitKart Backend API - Node.js Express",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "migrate": "typeorm migration:run -d dist/config/database.js",
    "migrate:create": "typeorm migration:create",
    "seed": "ts-node src/database/seeds/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:services": "jest --testPathPattern='services' --coverage",
    "test:controllers": "jest --testPathPattern='controllers' --coverage",
    "test:integration": "jest --testPathPattern='integration' --coverage",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "pg": "^8.8.0",
    "typeorm": "^0.3.12",
    "redis": "^4.6.0",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "axios": "^1.3.0",
    "joi": "^17.9.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "express-rate-limit": "^6.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.11.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/jest": "^29.4.0",
    "@types/morgan": "^1.9.5",
    "@types/bcryptjs": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "typescript": "^4.9.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "eslint": "^8.35.0",
    "jest": "^29.4.0",
    "ts-jest": "^29.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "jest-mock-extended": "^3.0.1",
    "jest-watch-typeahead": "^2.2.1"
  }
}
