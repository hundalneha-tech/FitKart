# .env.test - Test Environment Configuration

# Database (Test)
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=fitkart_test
DATABASE_USER=postgres
DATABASE_PASSWORD=postgres

# Redis (Test)
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT
JWT_SECRET=test_secret_key_12345678
JWT_EXPIRY=15m
REFRESH_TOKEN_EXPIRY=7d

# Environment
NODE_ENV=test
PORT=3001

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:3000

# Logging
LOG_LEVEL=debug

# Email (disabled in test)
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_USER=test
SMTP_PASSWORD=test

# Shopify (disabled in test)
SHOPIFY_STORE_URL=https://test.myshopify.com
SHOPIFY_ACCESS_TOKEN=test_token

# AWS (disabled in test)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=test_key
AWS_SECRET_ACCESS_KEY=test_secret
AWS_S3_BUCKET=fitkart-test

# Feature Flags
ENABLE_EMAIL_VERIFICATION=false
ENABLE_OTP=true
ENABLE_RATE_LIMITING=false
ENABLE_ANTI_CHEAT=true

# Test Settings
TEST_TIMEOUT=10000
TEST_USER_ID=test-user-1
TEST_ADMIN_ID=test-admin-1
